{
  "name": "user-flow-engine",
  "version": "1.0.0",
  "description": "An Engine for Customizable Scenarios",
  "keywords": ["flows", "scenarios", "customizable flows", "customizable scenarios"],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build:types": "tsc -p src/tsconfig.json && tsc-alias -p src/tsconfig.json",
    "build": "bin/build.mjs && npm run build:types",
    "prepublish": "npm run build"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "author": "Kenny Romanov",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-typescript": "^7.28.5",
    "@handlebars/parser": "^2.2.2",
    "@types/node": "^25.3.0",
    "esbuild": "^0.27.3",
    "esbuild-plugin-import-glob": "^0.1.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  },
  "dependencies": {}
}
